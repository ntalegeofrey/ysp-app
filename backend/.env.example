# Backend environment variables (.env) - copy to .env and fill in
# Database
DB_URL=jdbc:postgresql://localhost:5432/ysp_platform
DB_USERNAME=
DB_PASSWORD=

# JWT
JWT_SECRET=
JWT_TTL_SECONDS=7200

# Admin bootstrap (optional)
ADMIN_EMAIL=
ADMIN_PASSWORD=

# CORS
CORS_ORIGIN=http://localhost:3000

# SMTP (MailHog defaults for local dev)
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_AUTH=false
SMTP_STARTTLS=false
FROM_EMAIL=dev@ysp.local

# Branding
APP_LOGO_URL=
